{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "airbnb-typescript"
  ],
  "rules": {
    "comma-dangle": [2, "never"],
    "no-console": "off",
    "no-plusplus": "off",
    "no-multi-spaces": "off",
    "react/jsx-filename-extension": "off",
    "@typescript-eslint/camelcase": "off"
  }
}
